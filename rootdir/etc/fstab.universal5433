# Android fstab file.
#<src>														<mnt_point>	type>	<mnt_flags and options>																		<fs_mgr_flags>
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK
# data partition must be located at the bottom for supporting device encryption

/dev/block/platform/15540000.dwmmc0/by-name/BOOT            /boot       emmc	defaults                                                                                    recoveryonly
/dev/block/platform/15540000.dwmmc0/by-name/RECOVERY        /recovery   emmc	defaults                                                                                    recoveryonly
/dev/block/platform/15540000.dwmmc0/by-name/SYSTEM          /system     ext4	ro,noatime,errors=panic,noload                                                              recoveryonly
/dev/block/platform/15540000.dwmmc0/by-name/EFS             /efs        ext4	nosuid,nodev,noatime,noauto_da_alloc,discard,journal_async_commit,data=ordered,errors=panic wait,check
/dev/block/platform/15540000.dwmmc0/by-name/CACHE           /cache      ext4	noatime,nosuid,nodev,discard,noauto_da_alloc,journal_async_commit,errors=panic              wait,check
/dev/block/platform/15540000.dwmmc0/by-name/CACHE           /cache      f2fs    noatime,nosuid,nodev,nodiratime,inline_xattr,inline_data,data_flush                         wait,check
/dev/block/platform/15540000.dwmmc0/by-name/USERDATA        /data       ext4	noatime,nosuid,nodev,discard,noauto_da_alloc,journal_async_commit,errors=panic              wait,check,length=-20480,reservedsize=128M
/dev/block/platform/15540000.dwmmc0/by-name/USERDATA        /data       f2fs    noatime,nosuid,nodev,nodiratime,inline_xattr,inline_data,data_flush                         wait,check,length=-20480,reservedsize=128M

# Updater service
/dev/block/platform/15540000.dwmmc0/by-name/OTA             /misc       emmc	defaults																					defaults

# VOLD

/devices/15560000.dwmmc2/mmc_host/mmc1*                     auto        auto	defaults																					voldmanaged=sdcard:auto
/devices/15400000.usb/15400000.dwc3/xhci-hcd.4.auto/usb*    auto        auto	defaults																					voldmanaged=usb:auto

/dev/block/zram0                                            none        swap    defaults                                                                    zramsize=50%,notrim

